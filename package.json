{
  "title": "Landing Page",
  "name": "startbootstrap-landing-page",
  "version": "6.0.6",
  "scripts": {
    "dev": "npm run start:debug",
    "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
    "build:pug": "node scripts/build-pug.js",
    "build:assets": "node scripts/build-assets.js",
    "build:scripts": "node scripts/build-scripts.js",
    "build:scss": "node scripts/build-scss.js",
    "clean": "node scripts/clean.js",
    "start": "npm run build && node scripts/start.js",
    "start:debug": "npm run build && node scripts/start-debug.js",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "description": "A multilingual HTML landing page template built with Bootstrap",
  "keywords": [
    "css",
    "sass",
    "html",
    "responsive",
    "theme",
    "template",
    "multilingual",
    "bootstrap",
    "landing-page"
  ],
  "homepage": "https://startbootstrap.com/theme/landing-page",
  "bugs": {
    "url": "https://github.com/StartBootstrap/startbootstrap-landing-page/issues",
    "email": "feedback@startbootstrap.com"
  },
  "license": "MIT",
  "author": "Start Bootstrap",
  "contributors": [
    "David Miller (https://davidmiller.io/)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/StartBootstrap/startbootstrap-landing-page.git"
  },
  "dependencies": {
    "bootstrap": "5.3.2",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "autoprefixer": "10.4.16",
    "browser-sync": "2.29.3",
    "chokidar": "3.5.3",
    "concurrently": "8.2.2",
    "eslint": "^8.54.0",
    "globals": "^15.12.0",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "pug": "3.0.2",
    "sass": "1.69.5",
    "shelljs": "0.8.5",
    "typescript-eslint": "^8.14.0",
    "upath": "2.0.1"
  }
}
